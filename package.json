{
  "name": "@tuplo/dynoexpr",
  "description": "Expression builder for AWS.DynamoDB.DocumentClient",
  "version": "0.0.0-development",
  "repository": "git@github.com:tuplo/dynoexpr.git",
  "author": "Rui Costa",
  "license": "MIT",
  "keywords": [
    "aws",
    "amazon",
    "dynamodb",
    "database",
